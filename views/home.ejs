<%- include("partials/header"); -%>

<h1>Welcome to the Home Page</h1>

<!-- Employee Form -->
<form action="/employees" method="POST">
  <h2>Employee</h2>
  <label for="employee_id">Employee ID:</label>
  <input type="text" id="employee_id" name="employee_id" required><br>

  <label for="name">Name:</label>
  <input type="text" id="name" name="name" required><br>

  <label for="home_location">Home Location:</label>
  <input type="text" id="home_location" name="home_location" required><br>

  <label for="shift_opted">Shift Opted:</label>
  <input type="text" id="shift_opted" name="shift_opted" required><br>

  <label for="leaves">Preference for Leaves:</label>
  <input type="checkbox" id="leaves" name="leaves"><br>

  <label for="work_from_home">Preference for Work from Home:</label>
  <input type="checkbox" id="work_from_home" name="work_from_home"><br>

  <label for="cab_id">Cab ID:</label>
  <input type="text" id="cab_id" name="cab_id"><br>

  <label for="driver_id">Driver ID:</label>
  <input type="text" id="driver_id" name="driver_id"><br>

  <input type="submit" value="Submit">
</form>

<!-- Driver Form -->
<form action="/drivers" method="POST">
  <h2>Driver</h2>
  <label for="driver_id">Driver ID:</label>
  <input type="text" id="driver_id" name="driver_id" required><br>

  <label for="name">Name:</label>
  <input type="text" id="name" name="name" required><br>

  <label for="contact_number">Contact Number:</label>
  <input type="text" id="contact_number" name="contact_number" required><br>

  <label for="route">Route:</label>
  <input type="text" id="route" name="route" required><br>

  <label for="cab_id">Cab ID:</label>
  <input type="text" id="cab_id" name="cab_id"><br>

  <input type="submit" value="Submit">
</form>

<!-- Cab Form -->
<form action="/cabs" method="POST">
  <h2>Cab</h2>
  <label for="cab_id">Cab ID:</label>
  <input type="text" id="cab_id" name="cab_id" required><br>

  <label for="number_plate">Number Plate:</label>
  <input type="text" id="number_plate" name="number_plate" required><br>

  <label for="capacity">Capacity:</label>
  <input type="number" id="capacity" name="capacity" required><br>

  <label for="garage_location">Garage Location:</label>
  <input type="text" id="garage_location" name="garage_location" required><br>

  <input type="submit" value="Submit">
</form>

<!-- Holiday Form -->
<form action="/holidays" method="POST">
  <h2>Holiday</h2>
  <label for="holiday_id">Holiday ID:</label>
  <input type="text" id="holiday_id" name="holiday_id" required><br>

  <label for="date">Date:</label>
  <input type="text" id="date" name="date" required><br>

  <label for="description">Description:</label>
  <input type="text" id="description" name="description" required><br>

  <input type="submit" value="Submit">
</form>

<!-- Co-Employee Form -->
<form action="/coemployees" method="POST">
  <h2>Co-Employee</h2>
  <label for="employee_id">Employee ID:</label>
  <input type="text" id="employee_id" name="employee_id" required><br>

  <label for="co_employees">Co-Employees:</label>
  <input type="text" id="co_employees" name="co_employees" required><br>

  <input type="submit" value="Submit">
</form>

